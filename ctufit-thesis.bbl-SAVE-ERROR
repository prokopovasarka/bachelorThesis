      \strng{fullhashraw}{5bd878b44e3686b44ba42c507c5d5a7c}
      \strng{bibnamehash}{5bd878b44e3686b44ba42c507c5d5a7c}
      \strng{authorbibnamehash}{5bd878b44e3686b44ba42c507c5d5a7c}
